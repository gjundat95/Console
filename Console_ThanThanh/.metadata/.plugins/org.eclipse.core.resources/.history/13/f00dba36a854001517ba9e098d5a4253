package Test;

public class Main {

	public static void main(String[] args) 
	{
		int a=2;
		int b=3;
		int h =(a<b)? 2:1;
		System.out.println("Gia tri:"+h);
		
		start:
		for(int i=0; i< 50; i++)
		{
			for(int j =0; j<50; j++)
